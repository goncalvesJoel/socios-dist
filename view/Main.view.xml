<mvc:View
    controllerName="socios.controller.Main"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    
    <App>
        <Page title="Demo SAPUI5">
            <content>
                <VBox class="sapUiLargeMargin">

                    <Button text="Adicionar Item" press=".onAddItem"/>

                    <List id="myList" items="{
                        path: '/items'
                    }">
                        <StandardListItem title="{title}" />
                    </List>

                </VBox>
            </content>
        </Page>
    </App>
</mvc:View>